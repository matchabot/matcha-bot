{
  "name": "add-generator",
  "description": "âš™add a new command to your generator",
  "version": "1.0.0",
  "args": [
    {
      "name": "GeneratorName",
      "alias": "n",
      "type": "string"
    },
    {
      "name": "Description",
      "alias": "d",
      "type": "string"
    },
    {
      "name": "Version",
      "alias": "v",
      "type": "string",
      "default": "1.0.0"
    }
  ],
  "actions": [
    {
      "type": "template",
      "name": "Generate a matchabotbot.json file",
      "source": "matchabot.json.hbs",
      "target": "./_matchabot/{{kebabCase GeneratorName}}/matchabot.json"
    }
  ]
}
